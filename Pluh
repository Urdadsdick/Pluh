if game.PlaceId == 3101667897 then
    -- Load Orion Library
    local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

    -- Create Window
    local Window = OrionLib:MakeWindow({
        Name = "Goated Hub - Auto Tab",
        HidePremium = false,
        SaveConfig = true,
        ConfigFolder = "GoatedHub"
    })

    -- Add an "Auto" tab
    local autoTab = Window:MakeTab({
        Name = "Auto",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    -- You can add features to the Auto tab here, for example:
    -- autoTab:AddToggle({ Name = "Example Feature", Default = false, Callback = function(state) print(state) end })

    -- Initialize the Orion UI
    OrionLib:Init()
else
    warn("This script is not for this game.")
end
